# AGE CHECKER
adult_age=18
def age_check(age):
  if age>=adult_age:
    return True
  return False
def main():
  age:str=int(input("enter yoour age "))
  print(age_check(age))
if __name__ == "__main__":
    main()
    # GREETINGS
def greet(name):
  if name:
    return "Greetings "+ name + "!"
  return "name is necessary"
def main():
     name : str = input("What's your name? ")
     print(greet(name))
if __name__ == "__main__":
    main()

# range
def in_range(n, low, high):
      if n >= low and n <= high:
	      return True


def main():
  n = int(input("Enter a number: "))
  low = int(input("Enter a low number: "))
  high = int(input("Enter a high number: "))
  print(in_range(n, low, high))
# fruits
if __name__ == "__main__":
    main()
def stocks_fruit(fruit):
  fruit_count=["appke","mange","banana"]
  stock_count=[1,2,3]
  if fruit in fruit_count:
    index=fruit_count.index(fruit)
    return stock_count[index]
  else:
    return 0
def main():
  fruit=str(input("enter the name of fruit"))
  stocks=stocks_fruit(fruit)
  if stocks==0:
    print("this fruit is not available")
  else:
    print("this fruit is available with this numbers of fruits remains")
    print(stocks)
if __name__ == "__main__":
  main()
# subtract
def subtract_seven(num):
  num-=7
  return num
def main():
  num=float(input("enter the number"))
  print(subtract_seven(num))
  if __name__ == "__main__":
    main()
    # user info
def get_user_info():
    first_name: str = input("What is your first name?: ")
    last_name: str = input("What is your last name?: ")
    email_address : str = input("What is your email address?: ")
    return first_name, last_name, email_address
def main():
    user_data = get_user_info()
    print("Received the following user data:", user_data)
if __name__ == "__main__":
     main()
