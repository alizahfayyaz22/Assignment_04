# 1 program
joke:str="Here is a joke for you! Panaversity GPT - Sophia is heading out to the grocery store. A programmer tells her: get a liter of milk, and if they have eggs, get 12. Sophia returns with 13 liters of milk. The programmer asks why and Sophia replies: 'because they had eggs'"
prompt:str="what do yoou want"
sorry="sorry i only tell jokes"
def main():
  user_input:str = input(prompt)
  if "joke" in user_input:
    print(joke)
  else:
      print(sorry)

if __name__ == "__main__":
    main()
    # program 2
def main():
  number=int(input("enter a number to double"))
  print(f"the {number} double is",number*2)
  result=number*2
  while result <= 100:
    print(f"{number} double is {result} ")
    result=result*2
  print("we are not executing beyond this point beacause number is greater then hundred ")

if __name__ == "__main__":
    main()
# program 3
def main():
  i:int=10
  while i>0:
    print(i,end=" ")
    i=i-1

if __name__ == "__main__":
    main()

# program 4
import random
def main():
    secret_number: int = random.randint(1, 100)
    print("I am thinking of a number between 1 and 100...")
    guess = int(input("Enter a guess: "))
    while guess != secret_number:
        if guess < secret_number:
            print("Your guess is too low")
        else:
            print("Your guess is too high")
        print()
        guess: int = int(input("Enter a new guess: "))
    print(f"Congrats! The number was: " + {secret_number})

if __name__ == '__main__':
    main()

# program 5
import random

N_NUMBERS: int = 10
MIN_VALUE: int = 1
MAX_VALUE: int = 100
def main():
   for i in range(10):
    print(random.randint(MIN_VALUE,MAX_VALUE))
if __name__ == '__main__':
    main()
